{% extends 'base.html'%}
{% load static %}

{%block content%}
{% if user.is_authenticated %}
<div class="alert alert-success" role="alert">
    Votre adresse IP est :- {{client_ip}}
  </div>
{% else %}
<div class="alert alert-danger" role="alert">
    Connectez-vous pour voir votre adresse IP !
  </div>
{% endif %}
    
{%endblock content%}